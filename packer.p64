--[[pod_format="raw",created="2025-12-20 06:27:19",modified="2025-12-20 06:33:27",revision=17]]
--!run this, do not load and run!

cd("..")
notify(pwd())

notify("Deleting previous version")
rm("compressed.p64")

notify("Creating distro in RAM")
mkdir("/ram/distro.p64")

notify("Cloning distro")
cp(pwd(),"/ram/distro.p64")

notify("Removing compressed.p64 (safety remove)")
rm("/ram/distro.p64/compressed.p64")

notify("Removing packer.p64")
rm("/ram/distro.p64/packer.p64")

notify("Removing readme.md")
rm("/ram/distro.p64/readme.md")

notify("Removing LICENSE")
rm("/ram/distro.p64/LICENSE")

notify("Clearing metadata of /lib/head.lua")
store_metadata("/lib/head.lua",{metadata_format="none"})

notify("Copying to distro")
cp("/ram/distro.p64","compressed.p64")

notify("Clearing RAM")
rm("/ram/distro.p64")

notify("DONE!")
